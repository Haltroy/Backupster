<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <Nullable>enable</Nullable>
    <AssemblyName>Backupster</AssemblyName>
    <Authors>Haltroy</Authors>
    <Description>Ultimate open-source free backup software.</Description>
    <PackageProjectUrl>https://github.com/haltroy/Backupster</PackageProjectUrl>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Avalonia" Version="0.10.2" />
    <PackageReference Include="Avalonia.Desktop" Version="0.10.2" />
    <PackageReference Include="Avalonia.Diagnostics" Version="0.10.2" />
   </ItemGroup>
</Project>
